{
    "name": "giadc/json-api-response",
    "description": "A package for handling JSON API responses",
    "license": "MIT",
    "authors": [
        {
            "name": "David Hill",
            "email": "dhill2@gannett.com"
        },
        {
            "name": "Ryan Marganti",
            "email": "rmarganti@gannett.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/collections": "^1.6",
        "doctrine/orm": "^2.11",
        "giadc/json-api-request": "~3.0.0",
        "league/fractal": "^0.20",
        "symfony/http-kernel": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Giadc\\JsonApiResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
